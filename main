										// 	Image = "<img height=200px width=200px src='" + tmp_img + "'></img>";
										// 	input.plain_aspl_teacher = Image;
										// }
										img = Add_ASPL[ID == input.Select_Your_ASPL_Teachers].Logo_ASPL;
										if(img != null)
										{
											//Replacing the masked parameters with valid application specific parameters
											img = img.replaceAll("/sharedBy/appLinkName/",zoho.appuri);
											img = img.replaceAll("viewLinkName","ASPL_List");
											img = img.replaceAll("fieldName","ASPL_Image");
											img = img.replaceAll("<img ","<img height=100px width=100px ");
											input.plain_aspl_teacher = img;
										}
						)
					}
				}

			}
