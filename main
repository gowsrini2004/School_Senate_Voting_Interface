			Name_filtering1 as "Name filtering"
			{
				type =  form
				form = Teachers_Voting_Form
				record event = on add

				on user input of Select_Your_Department
				{
					actions 
					{
						custom deluge script
						(
											if(input.Select_Your_Department != null)
										{
											clear Select_Your_Name_Teacher;
											for each  record in Teachers_Details_Form[Department == input.Select_Your_Department]
											{
												if(record.Status == "Pending")
												{
													input.Select_Your_Name_Teacher:ui.add(record.Teacher_Name);
												}
											}
										}
						)
					}
				}

			}
			Hide_Field1 as "Hide Field"
			{
				type =  form
				form = Student_Details_Form
				record event = on add

				field rules
				{
					actions 
					{
						hide Voting_Status;
					}
				}

			}
			Update_Pending_Status_To_1 as "Update Pending Status To Voted"
			{
				type =  form
				form = Students_Voting_Form
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											// recorstudent_record = Student_Details_Form[Name == input.Your_Name && Class == input.Select_Your_Class && Section == input.Your_Sec && Role_no == input.Roll_Number];
										// recorstudent_record.Voting_Status="Voted";
										recorstudent_record = Student_Details_Form[Enter_Your_Admission_Number_New == input.Enter_Your_Admission_Number_VotingForm];
										recorstudent_record.Voting_Status="Voted";
						)
					}
				}

			}
			Update_pending_to_voted1 as "Update pending to voted"
			{
				type =  form
				form = Teachers_Voting_Form
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											teacher_record = Teachers_Details_Form[Teacher_Name == input.Select_Your_Name_Teacher && Department == input.Select_Your_Department];
										teacher_record.Status="Voted";
						)
					}
				}

			}
