						(
											disable Your_Name;
										disable Your_Sec;
										disable Roll_Number;
										disable Select_Your_Class;
										hide Vote_Your_SPL_Here;
										hide Vote_Your_ASPL_Here;
										hide Your_Details;
						)
					}
				}

			}
			Check_and_Load_The_Studen as "Check and Load The Student Details"
			{
				type =  form
				form = Students_Voting_Form
				record event = on add or edit

				on user input of Enter_Your_Admission_Number_VotingForm
				{
					actions 
					{
						custom deluge script
						(
											disable Your_Name;
										disable Your_Sec;
										disable Roll_Number;
										disable Select_Your_Class;
										hide Vote_Your_SPL_Here;
										hide Vote_Your_ASPL_Here;
										rec = Student_Details_Form[Enter_Your_Admission_Number_New == input.Enter_Your_Admission_Number_VotingForm];
										if(rec != null)
										{
											if(rec.Voting_Status == "Pending")
											{
												input.Your_Name = rec.Name;
												input.Your_Sec = rec.Section;
												input.Select_Your_Class = rec.Class;
												input.Roll_Number = rec.Role_no;
												show Vote_Your_SPL_Here;
												show Vote_Your_ASPL_Here;
												show Your_Details;
												hide To_Vote;
											}
											else if(rec.Voting_Status == "Voted")
											{
												alert "You Have Already Voted";
												hide Your_Details;
												show To_Vote;
											}
											else
											{
												alert "Invalid Token Entry";
												hide Your_Details;
												show To_Vote;
											}
										}
										// else
										// {
										// 	alert "Error";
										// 	input.Your_Name = "";
										// 	input.Your_Sec = "";
										// 	input.Select_Your_Class = "";
										// 	input.Roll_Number = 0;
										// }
						)
					}
				}

			}
			Hide_Field as "Hide Field"
			{
				type =  form
				form = Add_SPL
				record event = on add or edit

				on load
				{
					actions 
					{
						custom deluge script
						(
											hide Vote_Count_spl;
						)
					}
				}

			}
			Vote_Counter_For_Student as "Vote Counter For Student"
			{
				type =  form
				form = Students_Voting_Form
				record event = on add or edit

				on success
				{
					actions 
					{
						custom deluge script
						(
											spl_rec = Add_SPL[ID == input.Select_Your_SPL];
										aspl_rec = Add_ASPL[ID == input.Select_Your_ASPL];
										if(spl_rec != null && aspl_rec != null)
										{
											aspl_rec.Vote_Count_aspl=aspl_rec.Vote_Count_aspl + 1;
											spl_rec.Vote_Count_spl=spl_rec.Vote_Count_spl + 1;
										}
						)
					}
				}

			}
			Vote_Count_For_Teacher as "Vote Count For Teacher"
